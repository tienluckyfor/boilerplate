{
  "name": "ts-react-tailwindui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "formik": "^2.2.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "tailwind-capitalize-first-letter": "^1.0.4",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start-win": "kill -9 $(lsof -ti:3011) && set PORT=3011 && react-scripts start",
    "start-macos": "kill -9 $(lsof -ti:3011); PORT=3011 react-scripts start",
    "build": "react-scripts --max_old_space_size=4096 build",
    "after-build": "rm -rf after_build; mv build after_build",
    "build:local": "env-cmd -f .env.local npm run-script build; npm run-script after-build",
    "build:dev": "env-cmd -f .env.dev npm run-script build; npm run-script after-build",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.7",
    "react-router-dom": "^6.2.1",
    "tailwindcss": "^3.0.23",
    "yup": "^0.32.11"
  }
}
